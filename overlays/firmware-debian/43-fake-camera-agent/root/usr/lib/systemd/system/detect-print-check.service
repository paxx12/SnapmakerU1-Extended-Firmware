[Unit]
Description=Print Detection Service
After=capture-mipi-mpp.service fake-camera-agent.service
Requires=capture-mipi-mpp.service
ConditionPathExists=/dev/video11

[Service]
Type=simple
User=lava
ExecStart=/usr/local/bin/detect-rknn-yolo11.py --model /opt/lava/unisrv/camera_service/model/print_check.fp.rknn --labels noodle,residue,clean_bed --jpeg-sock /tmp/capture-mipi-jpeg.sock --sock /tmp/detect-print-check.sock
Restart=always
RestartSec=5
StartLimitBurst=3
StartLimitIntervalSec=60

[Install]
WantedBy=multi-user.target
